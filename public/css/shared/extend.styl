@-o-keyframes cyclefour
  cyclefour()

@-moz-keyframes cycleone
  cycleone()

@-moz-keyframes cycletwo
  cycletwo()

@-moz-keyframes cyclethree
  cyclethree()

@-moz-keyframes cyclefour
  cyclefour()

@keyframes cycleone
  cycleone()

@keyframes cycletwo
  cycletwo()

@keyframes cyclethree
  cyclethree()

