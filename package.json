{
  "name": "webuild.sg",
  "version": "2.2.23",
  "homepage": "http://webuild.sg",
  "keywords": "developer, designer, maker, open source, open events, open community",
  "private": true,
  "scripts": {
    "start": "node app",
    "test": "grunt travis --verbose && mocha test --recursive && mocha test --recursive --require blanket --reporter html-cov > coverage.html",
    "postinstall": "bower install && grunt build"
  },
  "dependencies": {
    "body-parser": "1.9.2",
    "buffer-crc32": "0.2.3",
    "compression": "1.2.0",
    "cookie-parser": "1.3.3",
    "cookie-signature": "1.0.5",
    "errorhandler": "1.2.2",
    "express": "4.10.1",
    "github": "0.2.2",
    "htmlstrip-native": "0.0.4",
    "ical-generator": "0.1.9",
    "jade": "1.7.0",
    "jsonfile": "2.0.0",
    "markdown": "0.5.0",
    "mess": "0.1.2",
    "moment-timezone": "0.2.4",
    "newrelic": "1.13.2",
    "passport": "0.2.1",
    "passport-auth0": "0.2.0",
    "prequest": "0.0.2",
    "promise": "6.0.1",
    "request": "2.47.0",
    "serve-favicon": "2.1.6",
    "word-overlap": "2.2.2"
  },
  "devDependencies": {
    "blanket": "1.1.6",
    "bower": "1.3.12",
    "chai": "1.9.2",
    "coveralls": "2.11.2",
    "grunt": "0.4.5",
    "grunt-bump": "0.0.16",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-csslint": "0.3.1",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-contrib-sass": "0.8.1",
    "grunt-contrib-uglify": "0.6.0",
    "grunt-jscs": "0.8.1",
    "mocha": "2.0.1",
    "mocha-lcov-reporter": "0.0.1"
  },
  "engines": {
    "node": "0.10"
  },
  "config": {
    "blanket": {
      "pattern": [
        "countdown/index.js",
        "events/eventbriteEvents.js",
        "events/facebookEvents.js",
        "events/index.js",
        "events/meetupEvents.js",
        "events/utils.js",
        "repos/index.js"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
